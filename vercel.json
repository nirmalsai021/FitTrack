{
  "name": "fittrack-frontend",
  "version": 2,
  "builds": [
    {
      "src": "fittrack-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/fittrack-frontend/$1"
    }
  ]
}